<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context=".SplaceScreen">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/qw"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#250035"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_fileName="start12.json"
        app:lottie_loop="true" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/w"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#250035"
        app:layout_constraintBottom_toTopOf="@+id/tt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qw"
        app:lottie_autoPlay="true"
        app:lottie_fileName="start1.json"
        app:lottie_loop="true" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/tt"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#250035"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_fileName="star.json"
        app:lottie_loop="true" />


</androidx.constraintlayout.widget.ConstraintLayout>